<view class="uni-goods-nav"><view class="uni-tab__seat"></view><view class="uni-tab__cart-box flex"><view class="flex uni-tab__cart-sub-left"><block wx:for="{{options}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['onClick',[index,'$0'],[[['options','',index]]]]]]]}}" class="flex uni-tab__cart-button-left uni-tab__shop-cart" bindtap="__e"><view class="uni-tab__icon"><block wx:if="{{item.icon=='dianpu'}}"><image class="image" src="../../../../static/dianpu.png"></image></block><block wx:else><block wx:if="{{item.icon=='kefu'}}"><image class="image" src="../../../../static/kefu.png"></image></block><block wx:else><block wx:if="{{item.icon=='star-filled'}}"><uni-icons vue-id="{{'681f5a6a-1-'+index}}" type="{{item.icon}}" size="20" color="#ffb33c" bind:__l="__l"></uni-icons></block><block wx:else><uni-icons vue-id="{{'681f5a6a-2-'+index}}" type="{{item.icon}}" size="20" color="#646566" bind:__l="__l"></uni-icons></block></block></block></view><text class="uni-tab__text">{{item.text}}</text><view class="flex uni-tab__dot-box"><block wx:if="{{item.info}}"><text class="{{['uni-tab__dot',(item.info>9)?'uni-tab__dots':'']}}" style="{{'background-color:'+(item.infoBackgroundColor?item.infoBackgroundColor:'#ff0000')+';'+('color:'+(item.infoColor?item.infoColor:'#fff')+';')}}">{{item.info}}</text></block></view></view></block></view><view class="{{['flex','uni-tab__cart-sub-right',(fill)?'uni-tab__right':'']}}"><block wx:for="{{buttonGroup}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['buttonClick',[index,'$0'],[[['buttonGroup','',index]]]]]]]}}" class="flex uni-tab__cart-button-right" style="{{'background:'+(item.backgroundColor)+';'+('color:'+(item.color)+';')}}" bindtap="__e"><text class="uni-tab__cart-button-right-text" style="{{'color:'+(item.color)+';'}}">{{item.text}}</text></view></block></view></view></view>