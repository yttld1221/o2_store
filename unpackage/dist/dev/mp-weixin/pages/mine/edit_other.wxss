
.line-name{
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
	width: 93vw;
	padding: 0 3.5vw;
	
	border-bottom: #f3f3f3 1px solid;
}
.line-name-title{
	width: 30vw;
	height: 55px;
	line-height: 55px;
	color: #bbbbbb;
}
.line-name-content{
	width: 63vw;
	height: 55px;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.space-line-bottom{
	padding: 25px 0 65px 0;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.to-push-2{
	width: 93vw;
	margin: 0 3.5vw;
	height: 55px;
	line-height: 55px;
	text-align: center;
	color: #ffffff;
	background-color: #ff812f;
	border-radius: 100px;
}

