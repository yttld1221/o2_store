<view class="tabbar"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{[(true)?'tabbar-item':'',(index==2)?'tabbar-item-push':'']}}" bindtap="__e"><view><block wx:if="{{current==index}}"><image class="{{[(true)?'img':'',(index==2)?'push':'']}}" mode="widthFix" src="{{item.selectedIconPath}}"></image></block><block wx:else><image class="{{[(true)?'img':'',(index==2)?'push':'']}}" mode="widthFix" src="{{item.iconPath}}"></image></block><block wx:if="{{current==index}}"><view class="text active">{{item.text}}<block wx:if="{{index==3&&$store.state.isRedTip}}"><text class="red-tip"></text></block></view></block><block wx:else><view class="text">{{item.text}}<block wx:if="{{index==3}}"><text class="red-tip"></text></block></view></block></view></view></block></view>